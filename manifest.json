{
	"manifest_version": 3,
	"name": "animetick-tweet",
	"version": "1.3.1",
	"description": "AnimetickのWatchボタンを押した際に別タブでツイート画面を表示させる",
	"content_scripts": [
		{
	 		"matches": ["http://animetick.net/*"],
	    	"js": [
	      		"content.js"
	    	]
	    },
		{
	 		"matches": ["http://animetick.net/anime/*"],
	    	"js": [
	      		"individual.js"
	    	]
	    }
	]
}